{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "appData": {
    "version": "1.2",
    "changelog": "Umstellung auf Datenbank und Bildanzeige"
  },
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/PizzaCalculator/favicon.ico",
          "/PizzaCalculator/index.html",
          "/PizzaCalculator/manifest.webmanifest",
          "/PizzaCalculator/*.css",
          "/PizzaCalculator/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/PizzaCalculator/assets/**",
          "/PizzaCalculator/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ]
}
